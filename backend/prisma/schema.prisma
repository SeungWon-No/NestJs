generator client {
  provider = "prisma-client-js"
  output ="../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// 테스트를 위한 임시 작성한 모델
model Test {
  id String @id @default(uuid())

  @@map("test")
}
